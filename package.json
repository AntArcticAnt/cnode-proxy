{
  "name": "cnode-proxy",
  "version": "1.0.0",
  "description": "Cnode Porxy",
  "main": "index.js",
  "scripts": {
    "lint": "eslint --ext .js --ext .jsx client/",
    "precommit": "npm run lint",
    "build:client": "webpack --mode development --config build/webpack.config.client.js",
    "build:server": "webpack --mode development --config build/webpack.config.server.js",
    "clear": "rimraf dist",
    "build": "npm run clear && npm run build:client && npm run build:server",
    "dev:server": "nodemon server/server.js",
    "dev:client": "cross-env NODE_ENV=development webpack-dev-server --mode development --config build/webpack.config.client.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gongzheng0720@bitbucket.org/gongzheng0720/cnode-proxy.git"
  },
  "author": "EricGong",
  "license": "ISC",
  "homepage": "https://bitbucket.org/gongzheng0720/cnode-proxy#readme",
  "dependencies": {
    "axios": "^0.18.0",
    "express": "^4.16.3",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "serve-favicon": "^2.5.0",
    "webpack": "^4.4.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "cross-env": "^5.1.4",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "html-webpack-plugin": "^3.1.0",
    "http-proxy-middleware": "^0.18.0",
    "husky": "^0.14.3",
    "memory-fs": "^0.4.1",
    "nodemon": "^1.17.3",
    "react-hot-loader": "^4.0.1",
    "webpack-cli": "^2.0.13",
    "webpack-dev-server": "^3.1.1",
    "webpack-merge": "^4.1.2"
  }
