{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "rules": {
    "global-require":[0],
    "react/jsx-filename-extension":[0],
    "import/first":[0],
    "jsx-a11y/anchor-is-valid":[0]
  }
}
